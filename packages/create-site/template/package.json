		"--node:@torpor/adapter-node": "^0.0.13",
		"--cloudflare:@torpor/adapter-cloudflare": "^0.0.21",
		"@torpor/build": "^0.1.20",
		"dotenv": "^16.5.0"
	},
	"devDependencies": {
		"@typescript/native-preview": "7.0.0-dev.20250914.1",
		"--node:drizzle-kit": "^0.31.1",
		"oxlint-tsgolint": "^0.2.0",
		"typescript": "^5.8.3",
