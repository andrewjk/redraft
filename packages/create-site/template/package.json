{
	"name": "@redraft/create-site",
	"version": "0.0.1",
	"description": "A Redraft site",
	"type": "module",
	"scripts": {
		"dev": "tb --dev",
		"check": "tsgo --noEmit && pnpm dlx oxlint --type-aware",
		"build": "tsgo --noEmit && tb --build",
