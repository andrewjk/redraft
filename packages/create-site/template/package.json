	"license": "ISC",
	"dependencies": {
		"--node:@redraft/adapter-node": "^0.1.3",
		"--cloudflare:@redraft/adapter-cloudflare": "^0.1.6",
		"@redraft/site": "^0.3.2",
		"--node:@torpor/adapter-node": "^0.1.2",
		"--cloudflare:@torpor/adapter-cloudflare": "^0.2.3",
		"@torpor/build": "^0.4.14",
		"dotenv": "^17.2.3"
	},
	"devDependencies": {
		"@typescript/native-preview": "7.0.0-dev.20251214.1",
		"--node:drizzle-kit": "^0.31.8",
		"oxlint-tsgolint": "^0.9.0",
		"typescript": "^5.9.3",
		"--cloudflare:wrangler": "^4.54.0"
	}
