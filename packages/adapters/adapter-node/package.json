{
	"name": "@redraft/adapter-node",
	"version": "0.1.2",
	"description": "A Redraft adapter for running on Node",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"import": "./dist/index.mjs"
		},
		"./adapter": {
			"types": "./dist/adapter.d.mts",
			"import": "./dist/adapter.mjs"
		}
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"check": "tsgo --noEmit && pnpm dlx oxlint --type-aware",
		"build": "tsgo --noEmit && tsdown"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.9.0",
	"dependencies": {
		"@redraft/adapter-core": "workspace:^",
		"@torpor/adapter-node": "^0.1.2",
		"@torpor/build": "^0.4.14",
		"drizzle-orm": "^0.45.0"
	},
	"devDependencies": {
		"@types/node": "^24.10.2",
		"@typescript/native-preview": "7.0.0-dev.20251209.1",
		"oxlint-tsgolint": "^0.8.4",
		"prettier": "^3.7.4",
		"tsdown": "0.17.2",
		"typescript": "^5.9.3"
	}
}
