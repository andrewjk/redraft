{
	"name": "@redraft/adapter-cloudflare",
	"version": "0.1.2",
	"description": "A Redraft adapter for deploying to Cloudflare Workers",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"import": "./dist/index.mjs"
		},
		"./adapter": {
			"types": "./dist/adapter.d.mts",
			"import": "./dist/adapter.mjs"
		}
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"check": "tsgo --noEmit",
		"check:todo": "tsgo --noEmit && pnpm dlx oxlint --type-aware",
		"build": "tsgo --noEmit && tsdown src/iife.ts --format iife && tsx ./scripts/process.ts && tsdown"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.9.0",
	"dependencies": {
		"@redraft/adapter-core": "workspace:^",
		"@torpor/adapter-cloudflare": "^0.2.3",
		"@torpor/build": "^0.4.11",
		"drizzle-orm": "^0.44.7"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20251119.0",
		"@types/node": "^24.10.1",
		"@typescript/native-preview": "7.0.0-dev.20251118.1",
		"oxlint-tsgolint": "^0.7.1",
		"prettier": "^3.6.2",
		"tsdown": "^0.16.5",
		"typescript": "^5.9.3"
	}
}
