{
	"name": "@redraft/adapter-cloudflare",
	"version": "0.0.7",
	"description": "A Redraft adapter for deploying to Cloudflare Pages",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./adapter": {
			"types": "./dist/adapter.d.ts",
			"import": "./dist/adapter.js",
			"require": "./dist/adapter.cjs"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"check": "tsc --noEmit",
		"build": "tsc --noEmit && tsup"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.9.0",
	"dependencies": {
		"@redraft/adapter-core": "workspace:^",
		"@torpor/adapter-cloudflare": "^0.0.21",
		"@torpor/build": "^0.1.20",
		"drizzle-orm": "^0.43.1"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250525.0",
		"@types/node": "^22.15.21",
		"prettier": "^3.5.3",
		"tsup": "^8.5.0",
		"typescript": "^5.8.3"
	}
}
