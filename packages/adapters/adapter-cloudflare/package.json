{
	"name": "@redraft/adapter-cloudflare",
	"version": "0.0.18",
	"description": "A Redraft adapter for deploying to Cloudflare Workers",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./adapter": {
			"types": "./dist/adapter.d.ts",
			"import": "./dist/adapter.js"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"check": "tsc --noEmit",
		"build": "tsc --noEmit && tsdown src/iife.ts --format iife && tsx ./scripts/process.ts && tsdown"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.9.0",
	"dependencies": {
		"@redraft/adapter-core": "workspace:^",
		"@torpor/adapter-cloudflare": "^0.2.1",
		"@torpor/build": "^0.4.0",
		"drizzle-orm": "^0.44.7"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20251014.0",
		"@types/node": "^24.9.2",
		"prettier": "^3.6.2",
		"tsdown": "0.15.12",
		"typescript": "^5.9.3"
	}
}
